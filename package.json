{
  "name": "@xiaoshuapp/theme",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoshuapp/theme.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild && run-s gen",
    "dev": "unbuild --stub && run-s gen",
    "gen": "run-p gen:*",
    "gen:manifests": "vite-node ./scripts/genManifest.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.1.1",
    "mkdist": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "sass": "^1.69.5",
    "unbuild": "^2.0.0",
    "vite": "^5.0.2",
    "vite-node": "^0.34.6",
    "vue": "^3.3.7"
  }
}
